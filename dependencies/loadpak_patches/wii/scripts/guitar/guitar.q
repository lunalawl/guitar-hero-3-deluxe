			Links = 0x71cded0d
			uselinkslots
			Heap = 0xf8899a28}
		SetPakManCurrentBlock map = 0xcf7e837c pak = (<0xd4935026>.0x19e4a436) block_scripts = 1
	endif

	if IsNGC
		CreatePakManMap \{map = 0x261ffed7
			folder = 'pak/flow_state/'
			Links = 0xcc9b068f
			uselinkslots
			Heap = 0xf8899a28}
	else
		CreatePakManMap \{map = 0x261ffed7
			folder = 'pak/flow_state/'
			Links = 0xe8e13ec2
			uselinkslots
			Heap = 0xf8899a28}
	endif
	if ((<0xd4935026>.ui_flow_manager_state = career_beat_game_fs) || (<0xd4935026>.ui_flow_manager_state = coop_career_beat_game_fs))
		SetPakManCurrentBlock map = 0x261ffed7 pak = (<0xd4935026>.0x675f4d9d) block_scripts = 1
	elseif ((<0xd4935026>.ui_flow_manager_state = bootup_sequence_fs) || (<0xd4935026>.ui_flow_manager_state = career_select_venue_fs) || (<0xd4935026>.ui_flow_manager_state = coop_career_select_venue_fs))
		SetPakManCurrentBlock map = 0x261ffed7 pak = (<0xd4935026>.0x675f4d9d) block_scripts = 1
	else
		start_flow_manager flow_state = (<0xd4935026>.ui_flow_manager_state)
	endif
	PushMemProfile \{'Characters'}

	MemInitHeap Name = 'heap_zones' Size = (<0xd4935026>.heap_size_zones) vram_size = <heap_size_zones_vram>
