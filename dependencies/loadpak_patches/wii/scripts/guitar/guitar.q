script guitar_startup 
	SpawnScriptNow \{start_legal_timer}
	StopRendering

	GetPlatform
	switch <Platform>
		case PS2
		case NGC
		<heap_size_globalpak> = (600 * 1024)
		<heap_size_globalpak_vram> = (0 * 1024)
		<heap_size_audio_vram> = (0 * 1024)
		<heap_size_audio> = (0 * 1024)
		<0x2e61b44a> = (531 * 1024)
		<0x2ec66d79> = (0 * 1024)
		<heap_size_musician> = (531 * 1024)
		<heap_size_musician_vram> = (0 * 1024)
		<heap_size_guitarist_anim> = (1700 * 1024)
		<heap_size_guitarist_anim_vram> = (0 * 1024)
		<heap_size_drummer_anim> = (64 * 1024)
		<heap_size_drummer_anim_vram> = (0 * 1024)
		<heap_size_vocalist_anim> = (700 * 1024)
		<heap_size_vocalist_anim_vram> = (0 * 1024)
		<0x5cb78d80> = (175 * 1024)
		<0x595ea109> = (175 * 1024)
		<heap_size_drumkit> = (175 * 1024)
		<heap_size_song> = (11 * 128 * 1024)
		<heap_size_instrument_vram> = (0 * 1024)
		<heap_size_song_vram> = (0 * 1024)
		<heap_size_zones> = (10 * 512 * 1024)
		<heap_size_zones_vram> = (0 * 1024)
		<heap_size_downloads> = (0 * 1024)
		<heap_size_downloads_vram> = (0 * 1024)
		default
		ScriptAssert \{'Unrecognized platform for heap setup'}
	endswitch
	if (<Platform> = NGC)
		<heap_size_globalpak> = (1600 * 1024)
		<heap_size_musician> = (900 * 1024)
		<0x2e61b44a> = (900 * 1024)
		<0x5cb78d80> = (200 * 1024)
		<0x595ea109> = (200 * 1024)
		<heap_size_drumkit> = (200 * 1024)
		<heap_size_zones> = ((11 * 512 * 1024) + (128 * 1024))
		<heap_size_guitarist_anim> = (2000 * 1024)
	endif
	PushMemProfile \{'Light Show Heap'}
	MemInitHeap Name = 'heap_lightshow' Size = (104 * 1024) vram_size = 0
	PopMemProfile
	PushMemProfile \{'Global Pak Heap'}
	MemInitHeap Name = 'heap_global_pak' Size = <heap_size_globalpak> vram_size = <heap_size_globalpak_vram>
	PopMemProfile
	PushMemProfile \{'Characters'}
	MemInitHeap Name = 'heap_musician1' Size = <0x2e61b44a> vram_size = <0x2ec66d79>
	MemInitHeap Name = 'heap_musician2' Size = <0x2e61b44a> vram_size = <0x2ec66d79>
	MemInitHeap Name = 'heap_musician3' Size = <heap_size_musician> vram_size = <heap_size_musician_vram>
	MemInitHeap Name = 'heap_musician4' Size = <heap_size_musician> vram_size = <heap_size_musician_vram>
	MemInitHeap Name = 'heap_musician1_anim' Size = <heap_size_guitarist_anim> vram_size = <heap_size_guitarist_anim_vram>
	MemInitHeap Name = 'heap_musician2_anim' Size = <heap_size_guitarist_anim> vram_size = <heap_size_guitarist_anim_vram>
	MemInitHeap Name = 'heap_musician3_anim' Size = <heap_size_drummer_anim> vram_size = <heap_size_musician_vram>
	MemInitHeap Name = 'heap_musician4_anim' Size = <heap_size_vocalist_anim> vram_size = <heap_size_musician_vram>
	MemInitHeap Name = 'heap_instrument1' Size = <0x5cb78d80> vram_size = <heap_size_instrument_vram>
	MemInitHeap Name = 'heap_instrument2' Size = <0x5cb78d80> vram_size = <heap_size_instrument_vram>
	MemInitHeap Name = 'heap_instrument3' Size = <heap_size_drumkit> vram_size = <heap_size_instrument_vram>
	MemInitHeap Name = 'heap_instrument4' Size = <0x595ea109> vram_size = <heap_size_instrument_vram>
	PushMemProfile \{'Game Heap'}
	if IsNGC
		MemInitHeap Name = 'heap_game' Size = ((64 * 1024) + (20 * 256 * 1024)) vram_size = 0
	else
		MemInitHeap Name = 'heap_game' Size = ((64 * 1024) + (12 * 256 * 1024)) vram_size = 0
