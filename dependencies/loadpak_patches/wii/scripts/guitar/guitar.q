		SetPakManCurrentBlock \{map = 0xcf7e837c
			pak = NONE
			block_scripts = 1}
		DestroyPakManMap \{map = 0xcf7e837c}
	endif
	0x41688f4f \{0xf8899a28}
	heap_size_game = <Size>
	MemDeleteHeap \{Name = 'heap_game'}
	if (<0x19e4a436> = 0)
		return 0xd4935026 = {
			0x675f4d9d = <0x675f4d9d>
			0x296bb6d1 = <0x296bb6d1>
